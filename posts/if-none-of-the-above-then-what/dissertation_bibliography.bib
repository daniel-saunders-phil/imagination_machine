@article{klein2010,
  title = {Images Are Not the Evidence in Neuroimaging},
  author = {Klein, Colin},
  date = {2010},
  journaltitle = {British Journal for the Philosophy of Science},
  volume = {61},
  number = {2},
  pages = {265--278},
  issn = {14643537},
  doi = {10.1093/bjps/axp035},
  abstract = {fMRI promises to uncover the functional structure of the brain. I argue, however, that pictures of 'brain activity' associated with fMRI experiments are poor evidence for functional claims. These neuroimages present the results of null hypothesis significance tests performed on fMRI data. Significance tests alone cannot provide evidence about the functional structure of causally dense systems, including the brain. Instead, neuroimages should be seen as indicating regions where further data analysis is warranted. This additional analysis rarely involves simple significance testing, and so justified skepticism about neuroimages does not provide reason for skepticism about fMRI more generally. © 2009 The Author. Published by Oxford University.}
}

